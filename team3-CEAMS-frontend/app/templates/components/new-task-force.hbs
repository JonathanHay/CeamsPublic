<div class="ui middle aligned center aligned grid" id ="createElement">
<div class="six wide column">
  <div class="row">
    <div class="header">
      Adding new task force
    </div>
    <div class="content">
      <div class="ui form">
        <div class="field">
          <label>Task Force Name</label>
          {{input type="text" cols="50" rows="1" value = title placeholder="add title"}}
        </div>
      </div>
    </div>
    <br>
    <div class="actions">
      {{#link-to "task-force-management"}}
        <div class="ui black deny button">
          Cancel
        </div>
      {{/link-to}}
      {{#link-to "task-force-management"}}
        <button onclick={{action "add" placeholder}} class="ui positive right labeled icon button">
          Save
          <i class="checkmark icon"></i>
        </button>
      {{/link-to}}
    </div>
  </div>
</div>
</div>